<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.thekelvinliu.launch-alfred</string>
  <key>EnvironmentVariables</key>
  <dict>
    <key>app</key>
    <string>Alfred 3.app</string>
  </dict>
  <key>ProgramArguments</key>
  <array>
    <string>sh</string>
    <string>-c</string>
    <string>[ -r &quot;$app&quot; ] &amp;&amp; sleep 8 &amp;&amp; open -a &quot;$app&quot;</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>WorkingDirectory</key>
  <string>/Applications</string>
</dict>
</plist>
