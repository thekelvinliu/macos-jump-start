<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.thekelvinliu.launch-alfred</string>
  <key>ProgramArguments</key>
  <array>
    <string>sh</string>
    <string>-c</string>
    <string>app=&quot;Alfred 3.app&quot; &amp;&amp; [ -r &quot;$app&quot; ] &amp;&amp; sleep 8 &amp;&amp; open -a &quot;$app&quot; &amp;&amp; unset app</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>WorkingDirectory</key>
  <string>/Applications</string>
</dict>
</plist>
