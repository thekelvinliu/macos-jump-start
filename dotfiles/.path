# specific PATH environment variable setup

# get a common execution environment
MJS_BASE=${MJS_BASE:-"$HOME/macos-jump-start"}
. "$MJS_BASE/common.sh"

# add user directory bin
PATH="$HOME/bin:$PATH"

# add homebrew if it has been installed
if has_brew; then
  PATH="$(brew --prefix)/bin:$PATH"
fi

# export
export PATH
