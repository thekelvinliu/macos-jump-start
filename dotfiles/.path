# shellcheck shell=bash
# vim: ft=sh
# specific PATH environment variable setup

# add default homebrew bin
PATH=/usr/local/bin:$PATH

# add unversioned python3
PATH=/usr/local/opt/python/libexec/bin:$PATH

# add rust toolchain
PATH=$HOME/.cargo/bin:$PATH

# add home directory bin
PATH=$HOME/bin:$PATH

# export
export PATH
